<header class="password-header text-center" data-section-id="{{ section.id }}" data-section-type="password-header">
 
</header>

{% schema %}
{
  "name": "Header",
  "settings": [
    {
      "type": "image_picker",
      "id": "logo",
      "label": "Logo image"
    },
    {
      "type": "text",
      "id": "logo_max_width",
      "label": "Custom logo width (in pixels)",
      "default": "100"
    }
  ]
}
{% endschema %}
